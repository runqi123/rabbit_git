<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rabbit.dao.AreacodeMapper">
    <select id="selectAreacode" resultType="string" parameterType="string">
       SELECT
	`desc`

	FROM
	asrabbit.apo_areacode
	WHERE apo_areacode.zone=#{zone}
    </select>
</mapper>