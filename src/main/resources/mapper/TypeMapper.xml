<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rabbit.dao.TypeMapper">
    <select id="getType" resultType="Type">
        SELECT
            typeId,
            typeContent,
            typeImg
            FROM
            asrabbit.type
		</select>

</mapper>